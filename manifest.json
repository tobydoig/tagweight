{
  "name": "TagWeight",
  "version": "1.0",
  "description": "Displays a network graph of the resources loaded for various tags on a page",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png"
    },
    "default_title": "TagWeiht",
    "default_popup": "browserAction.html"
  },
  "permissions": [
    "tabs",
    "debugger"
  ]
}
